# Janus 多账户配置文件

# 账户列表
accounts:
  - name: "webull_personal"       # 唯一实例别名 (Gateway Name)
    broker: "webull"              # 经纪商类型 (决定使用哪个 Gateway 类)
    app_key: "YOUR_APP_KEY"
    app_secret: "YOUR_APP_SECRET"
    region_id: "us"

  - name: "webull_work"           # 另一个 Webull 账户
    broker: "webull"
    app_key: "YOUR_APP_KEY"
    app_secret: "YOUR_APP_SECRET"
    region_id: "us"

  - name: "ib_main"               # IB 账户
    broker: "ib"
    host: "127.0.0.1"
    port: 7497
    client_id: 1
    account: ""

# RPC 服务配置
rpc:
  rep_address: "tcp://*:2014"
  pub_address: "tcp://*:4102"

# 默认设置
default_account: "webull_personal"
history_file: ".janus_history"
