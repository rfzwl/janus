# Janus 多账户配置文件

# 账户列表
accounts:
  - name: "webull_personal"       # 唯一实例别名 (Gateway Name)
    broker: "webull"              # 经纪商类型 (决定使用哪个 Gateway 类)
    app_key: "YOUR_APP_KEY"
    app_secret: "YOUR_APP_SECRET"
    region_id: "us"
    trade_events:
      enabled: true
      host: ""                    # optional override (default from SDK)
      debounce_seconds: 1.0
      health_check_seconds: 60
      stale_seconds: 300
      startup_grace_seconds: 30
      health_log_interval: 300

  - name: "webull_work"           # 另一个 Webull 账户
    broker: "webull"
    app_key: "YOUR_APP_KEY"
    app_secret: "YOUR_APP_SECRET"
    region_id: "us"
    trade_events:
      enabled: true
      host: ""
      debounce_seconds: 1.0
      health_check_seconds: 60
      stale_seconds: 300
      startup_grace_seconds: 30
      health_log_interval: 300

  - name: "ib_main"               # IB 账户
    broker: "ib"
    host: "127.0.0.1"
    port: 7497
    client_id: 1
    account: ""
    ib_market_data:
      default_symbols: ["AAPL"]
      bar_size_seconds: 5
      what_to_show: "TRADES"
      use_rth: false

# RPC 服务配置
rpc:
  rep_address: "tcp://*:2014"
  pub_address: "tcp://*:4102"

# 默认设置
default_account: "webull_personal"
history_file: ".janus_history"
